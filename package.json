{
  "name": "action-update-api-management-openapi",
  "version": "0.1.0",
  "private": false,
  "description": "Github action task for updating Azure API Management API with an OpenAPI schema file",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run test && npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OssiPesonen/action-update-api-management-openapi.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Ossi Pesonen",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "axios": "^0.26.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/parser": "^4.14.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^7.18.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^24.9.0",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "prettier": "2.2.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.1.3"
  }
}
